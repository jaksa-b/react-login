    "@types/react-router-dom": "^5.1.7",
    "emotion": "^10.0.27",
    "history": "^5.0.0",
    "lorem-ipsum": "^2.0.3",
    "luxon": "^1.25.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
